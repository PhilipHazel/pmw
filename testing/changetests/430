Key B$
Time C
MIDItempo 80
Tripletfont italic
Justify top left right

*define 3a()  [slur/a] {&&1-&&2-&&3-}; [endslur]

[stave 1 treble 2 tripletize]
[nocount nocheck triplets above nobracket] 
&3a(/u5 d,e,d) &3a(/u4 c,d,b`) | @0
[triplets off]
f.r=d=; &3a(e,f,g) &3a(f,g,e) | d.r=f=; &3a(g,f,g) &3a(e,d,c) | @2
&3a(f,e,f,) &3a(d,c,b`) &3a(e,f,e) &3a(d,c,d) | @3
[nocheck] C :) | 
[endstave]

[stave 2 treble 1 tripletize]
[nocheck] f-.f=;f-.b= | a-.b=;c'-.d'=; g-.g=;c'-.c'= | @1
b-.a=;b-.d'=; b.r=c'= | c'.r=f=; g-.f=;f-.b= | @3
[nocheck] a-.g=f :) | @4
[endstave]

[stave 3 alto 1 tripletize]
[nocheck] b`-.b`=; f-.%e= | cf-.f=;$)e-.e=; f-.a= | @1
f-.e=; f-.f=; e.r=g= | f.r=f=;e-.c=; b`-.f= | @3
[nocheck] ff`-.a`= :) | @4
[endstave]

[stave 4 bass 0]
[tripletize nocheck] b`-.b=; a-.g= | f-.g=; a-.b=; c'-.b=; a-.f= | @1
b-.c'=; d'-.b= e'.r=e= | d.r=d=; c-.a=; b-.b`= | @3
[nocheck] f-.e=;d-.c= :) | @4
[endstave]


[newmovement]
[stave 1 treble 2 triplets off]
&3a(f,e,f,) &3a(d,c,b`) &3a(e,f,e) &3a(d,c,d) |
&3a(f,e,f,) &3a(d,c,b`) &3a(e,f,e) &3a(d,c,d) |
[endstave]

[stave 2 bass 0]
f-.g=; [tripletize on] a-.b=; c'-.b=; [tripletize off] a-.f= |
[tripletize] (f-.a-.)(g=b); a-.b=; c'-.b=; a-.f= |
[endstave]

