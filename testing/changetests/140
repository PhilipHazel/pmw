[newmovement]
systemgap 50
nocheck                   
unfinished
notime
heading ""

draw blip1
  linegapx linegapy moveto
  0 linegapx 2 mul lineto
  linegapx neg linegapy neg lineto
  0.3 setlinewidth stroke
  originx originy
enddraw  

draw blip2
  linegapx linegapy moveto
  0 linegapx 2 mul lineto
  linegapx neg linegapy neg lineto
  0.3 setlinewidth stroke
  
  originy sub exch originx sub
  dup 3 1 roll exch
  moveto
  2 div dup -15 3 -1 roll -15 0 0 curveto
  stroke
enddraw  

draw text
  0 -2 moveto
  "abc"/c show
enddraw  

draw ped
  0 0 moveto "\**163\ "show
enddraw  

draw vert
  0 0 moveto 0 10 lineto stroke
enddraw  

